        <!-- <%- include('components/modal.ejs') -%> -->

        <div style="display: flex;flex-direction:column;gap:24px;">
            <% cars.forEach(car => { %>
                <div>
                    <p><%= car.id %></p>
                    <p><%= car.name %></p>
                    <p><%= car.rent_per_day %></p>
                    <p><%= car.size %></p>
                    <p><%= car.image %></p>
                    <form action="/cars/delete/<%= car.id %>">
                        <button type="submit">delete</button>
                    </form>
                    <form action="/cars/update/<%= car.id %>">
                        <button type="submit">delete</button>
                    </form>
                </div>
            <% }) %>
        </div>
    </body>
